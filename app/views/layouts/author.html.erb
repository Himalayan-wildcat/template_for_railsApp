<!DOCTYPE html>
<html>
  <head>
    <title>TemplateForRailsapp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <%= render 'layouts/navbar' %>  
    <div class="container <%= controller_name %> <%= action_name %>">
     <%= yield %>
     </br>
     </br>
     <div class="col-md-10 offset-md-1 col-lg-8 offset-lg-2 text-xs-center"><%= debug(params) if Rails.env.development?  %></div>     
    </div>
  </body>
</html>
